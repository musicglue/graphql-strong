{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "interface-name": [
      false
    ],
    "max-line-length": [
      false
    ],
    "no-console": false,
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ]
  },
  "rulesDirectory": []
}
